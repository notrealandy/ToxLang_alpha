let x int >> 7
let y int >> 3 + 2 * (x - 4)
let msg string >> "Hello, Tox!"
let flag bool >> x > 5 && y < 10

fnc main () >> void {
    log(msg)
    if flag {
        log("Flag is true!")
    } elif x == 7 || y == 7 {
        log("One of the values is 7")
    } else {
        log("No condition met")
    }
    let result int >> add()
    log(result)
    modifyX()
    return nil
}

fnc add () >> int {
    return x + y
}

fnc modifyX () >> void {
    log(x)
    x >> x + 5
    log(x)
}